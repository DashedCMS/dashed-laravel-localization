{
    "name": "dashed/dashed-localization",
    "description": "Easy localization for Laravel",
    "keywords": [
        "localization",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/dashed/laravel-localization",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "info@dashed.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3|^8.4",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^10.1",
        "phpunit/phpunit": "^10.0|^11.0",
        "filament/upgrade": "^4.0"
    },
    "suggest": {
        "ext-intl": "*"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "classmap": [],
        "psr-0": {
            "Dashed\\LaravelLocalization": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dashed\\LaravelLocalization\\LaravelLocalizationServiceProvider"
            ],
            "aliases": {
                "LaravelLocalization": "Dashed\\LaravelLocalization\\Facades\\LaravelLocalization"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
